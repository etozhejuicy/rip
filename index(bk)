<script>
    // Adblock
    function helpUsImproveSite() {
        var height = document.documentElement.clientHeight;
        var table = document.getElementById("table");

        // table.style.marginTop = $(window).scrollTop() + 'px';
        // table.style.height = (height) + 'px';

        if(height < 600)
        {
            // document.getElementById('img').style.height = (height - 13) + 'px';
        }

        document.getElementById("kalambur").style.display = 'block';
        document.documentElement.style.overflow = 'hidden';
    }

    var timerSec = 15;
    function start_timer() {
        var seconds = document.getElementById("seconds");
        var close_interval = setInterval(function()
        {
            if (timerSec == 1) {
                clearInterval(close_interval);
                closePopup();
                return;
            }
            --timerSec;
            seconds.innerHTML = timerSec;
        }
        , 1000);
    }

    function closePopup() {
        document.getElementById("kalambur").style.display = 'none';
        document.documentElement.style.overflow = 'auto';
    }

    // Должен работать только если включены куки
    if (navigator.cookieEnabled)
    {
            }
</script>


